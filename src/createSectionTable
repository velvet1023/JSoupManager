CREATE TABLE section (
  section_id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  term TEXT NOT NULL,
  crn TEXT NOT NULL,
  discipline TEXT,
  courseNumber TEXT,
  section TEXT,
  type TEXT,
  title TEXT,
  hours TEXT,
  days TEXT,
  time TEXT,
  room TEXT,
  instructor TEXT,
  endDate TEXT,
  messages TEXT,
  maximumEnrollment TEXT,
  seatsAvailable TEXT,
  department TEXT,
  url TEXT);